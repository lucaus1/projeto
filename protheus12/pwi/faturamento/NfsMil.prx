					IF SUBSTR(SUBSTR(xDESC_PRO[I],41,41),41,1)<>SPACE(1).AND.SUBSTR(SUBSTR(xDESC_PRO[I],41,41),40,1)<>SPACE(1)
						TRACO:="-"
					ELSE
						TRACO:=" "
					ENDIF                         `
					
					alerta(xDESC_PRO[I])
					
					@ nLin,  20  PSAY SUBSTR(xDESC_PRO[I],41,40)+TRACO //Potencia 18 para 20
